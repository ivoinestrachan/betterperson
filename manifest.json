{
  "manifest_version": 2,
  "name": "Better Person",
  "version": "1.0",
  "description": "Become a better person",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["Content.js"]
    }
  ],
  "browser_action": {
    "default_popup": "Popup.html",
    "default_title": "Better Person"
  },
  "permissions": [
    "https://www.youtube.com/*"
  ]
}
